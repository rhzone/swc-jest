{
  "compilerOptions": {
    "module": "esnext",
    "lib": ["ESNext", "ES2015.Iterable"],
    "moduleResolution": "Node",
    "strictNullChecks": true,
    "strict": true,
    // "noImplicitAny": false,
    "noImplicitThis": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "target": "ESNext",
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
  },
  "include": ["./src", "global.d.ts"],
}